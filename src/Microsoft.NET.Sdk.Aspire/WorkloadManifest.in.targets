<Project>

  <Import
    Project="Sdk.targets" Sdk="Aspire.Hosting.Sdk"
    Condition=" ('$(IsAspireHost)' == 'true') and ('$(SkipAspireWorkloadManifest)' != 'true') "
  />

  <Import
    Project="Sdk.targets" Sdk="Aspire.Hosting.Orchestration"
    Condition=" ('$(IsAspireHost)' == 'true') and ('$(SkipAspireWorkloadManifest)' != 'true') "
  />

  <Import
    Project="Sdk.targets" Sdk="Aspire.Dashboard"
    Condition=" ('$(IsAspireHost)' == 'true') and ('$(SkipAspireWorkloadManifest)' != 'true') "
  />

</Project>
